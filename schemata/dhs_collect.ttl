    [ sh:path dcterms:title              ; sh:minCount 1 ; rdfs:comment:"The official title for the datset"@en; dhs:excelWidth 100; a sh:PropertyShape ; ],
    [ sh:path dcterms:description        ; sh:minCount 1 ; dhs:excelWidth 100; a sh:PropertyShape ; ],
    [ sh:path dcat:keyword               ; sh:minCount 0 ; dhs:excelWidth 100; a sh:PropertyShape ; ],
    [ sh:path dcterms:publisher          ; sh:minCount 0 ; dhs:excelWidth  30; a sh:PropertyShape ; ],
    [ sh:path usg:accessLevel            ; sh:minCount 1 ; dhs:excelWidth  30; a sh:PropertyShape ; ],
    [ sh:path usg:metadataClassificaiton ; sh:minCount 1 ; dhs:excelWidth  30; a sh:PropertyShape ; ],
