<http://resources.data.gov/resources/dcat-us/#>
    a owl:Ontology ;
    dct:contributor [
        sdo:affiliation [
            foaf:homepage <http://dhs.gov> ;
        foaf:name "U.S. Department of Homeland Security" ;
        ] ;
    foaf:name "Simson Garfinkel" ;
    ] ;
